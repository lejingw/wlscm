
<form id="frm" action="moveCmd.vm" method="post">
	<input type='hidden' id='user_action' name='user_action' value='selectMoveCmd'/>
    <table class="control">
    	<tr>
			<td>
				<input type='button' value='查询' class='btn' id="btnQueryShow" />
			</td>
		</tr>
    </table>
	<div class="scroll2">
    <table class='list' style='width:1800px;'>
    	<thead>
			<tr>
                <th width='45px'><input type='checkbox' id='chkall'/></th>
                <th width='120px'>单据编号</th>
                <th width='90px'>业务日期</th>
                <th width='180px'>调出组织</th>
                <th width='60px'>调出仓库</th>
                <th width='180px'>调入组织</th>
                <th width='60px'>调入仓库</th>
				<th width='60px'>件数合计</th>
				<th width='60px'>重量合计</th>
        		<th width='120px'>创建时间</th>
        		<th width='90px'>创建人</th>
        		<th width='120px'>修改时间</th>
        		<th width='90px'>修改人</th>
            </tr>
        </thead>
        <tbody id="tbl">
		#foreach($item in $form.pager.pageData)
			<tr>
				<td>
					<input type='checkbox' name='chk' value='$!{item.headid}'/>
					<input type="hidden" name="headid" value="$!item.headid"/>
					<input type="hidden" name="billno" value="$!item.billno"/>
					<input type="hidden" name="ornaCodes" value="$!item.ornaCodes"/>
					$velocityCounter
				</td>
				<td>$!item.billno</td>
				<td>$!item.dodate</td>
				<td>$!BdCommon.getOrgName($!item.outOrgId)</td>
				<td>$!DictUtil.getValue('invcode', $!item.outStockId)</td>
				<td>$!BdCommon.getOrgName($!item.inOrgId)</td>
				<td>$!DictUtil.getValue('invcode', $!item.inStockId)</td>
				<td>$!item.sumCount</td>
				<td>$!StringUtil.formatDouble($!item.sumWeight, 2)</td>
				<td>$!item.createDate</td>
				<td>$!BdCommon.getUserName($!item.createId)</td>
				<td>$!item.updateDate</td>
				<td>$!BdCommon.getUserName($!item.updateId)</td>
			</tr>
		#end
    	</tbody>
    </table>
	</div>
<script>
createPagingToolbar('frm', $!form.pager.start, $!form.pager.limit, $!form.pager.totalCount);
</script>
</form>
<script>
jQuery(function(){
	checkTable('tbl', 'chkall', 'chk', false);
	initQuery2('MC01', "frm", 'btnQueryShow');
});
function getValues(){
	var idx = getSelectIndex('chk');
	if(-1 == idx)
		return null;
	var result = {
			moveCmdId       :$n("headid")[idx].value,
            moveCmdNo       :$n("billno")[idx].value,
            ornaCodes     	:$n("ornaCodes")[idx].value
		};
	return result;
}
</script>