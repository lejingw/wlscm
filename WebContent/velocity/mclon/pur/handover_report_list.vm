#set($cssfiles = ["$StringUtil.getContextPath()/script/mclon/calendar/calendar.css"])
#set($jsfiles = ["$StringUtil.getContextPath()/script/mclon/calendar/calendar.js",
	"$StringUtil.getContextPath()/dwr/interface/BdCommonDwr.js",
	"$StringUtil.getContextPath()/dwr/interface/HandoverDwr.js?201203201723.js"])
<form id="frm" name="frm" method="post" action="handover.vm">
	<input type="hidden" id="user_action" name="user_action" value="reportList"/>
	<input type="hidden" id="type" name="type" value="$!form.condition.get('type')"/>
    <table class="title">
    	<tr><td>交接单#if($!form.message)[<font color='red'>$!form.message</font>]#end</td></tr>
    </table>
	#set($cond = $form.condition)
	#set($billno = $!cond.get('billno'))
	#set($orgId = $!cond.get('orgId'))
	#set($createId = $!cond.get('createId'))
	#set($startDate = $!cond.get('startDate'))
	#set($endDate = $!cond.get('endDate'))
	#set($verdorId = $!cond.get('verdorId'))
	#set($status = $!cond.get('status'))
	#set($isBill = $!cond.get('isBill'))
	#set($dotype = $!cond.get('dotype'))
    <table class="control">
    	
    </table>
<div class="scroll">
    <table class='list' style='width:1900px'>
    	<thead>
			<tr>
                <th width='45px'><input type='checkbox' id='chkall'/></th>
				<th style='width:60px;'>状态</th>
                <th style='width:120px;'>单据编号</th>
                <th style='width:120px;'>来源单号</th>
				<th style='width:90px;'>来源类型</th>
        		<th style='width:180px;'>组织</th>
				<th style='width:90px;'>业务类型</th>
				<th style='width:90px;'>业务时间</th>
				<th style='width:180px;'>供应商</th>
				<th style='width:60px;'>总数量</th>
				<th style='width:90px;'>是否结算</th>
				<th style='width:60px;'>收票</th>
				<th width="180px">备注</th>
        		<th style='width:120px;'>创建时间</th>
        		<th style='width:90px;'>创建人</th>
				<th width="90px">修改人</th>
        		<th width="120px">修改时间</th>
            </tr>
        </thead>
        <tbody id="tbl">
		#foreach($map in $form.pager.pageData)
			<tr>
				<td><input type='checkbox' name='chk' />$velocityCounter</td>
				<td><a href="javascript:viewReviewLog('JJ', '$!{map.billid}')">$!DictUtil.getValue('status', $!map.status)</a></td>
				<td><a href=''>$!{map.billno}</a></td>
                <td><a href=''>$!StringUtil.subString($!map.sourceNo, 14)</a></td>
				<td>$!DictUtil.getValue('billcode', $!map.sourceType)</td>
				<td>$!BdCommon.getOrgName($!map.orgId)</td>
				<td>$DictUtil.getValue("purbiztype", $!map.dotype)</td>
                <td>$!map.dodate</td>
				<td><input type='text' readonly class='none' value="$!BdCommon.getVendorName($!map.verdorId)" style='width:180px;'></td>
				<td>$!map.sumNum</td>
				<td style="color:blue;">#if($map.isCheck=="1")已结算#end</td>
				<td style="color:blue;">#if($map.isBill=="1")已收票#end</td>
				<td><input type='text' readonly class='none' value="$!map.memo" style='width:180px;'></td>
				<td>$!map.createDate</td>
				<td>$!BdCommon.getUserName($!map.createId)
					<input type="hidden" name="status_list" value="$!map.status"/>
					<input type="hidden" name="billid" value="$!map.billid"/>
				</td>
				<td>$!BdCommon.getUserName($!map.updateId)</td>
				<td>$!map.updateDate</td>
			</tr>
		#end
    	</tbody>
    </table>
</div>
<script>
createPagingToolbar('frm', $!form.pager.start, $!form.pager.limit, $!form.pager.totalCount);
</script>
</form>
<script>

jQuery(function(){
	initCalendarArr(["startDate", "endDate"]);
	checkTable('tbl', 'chkall', 'chk', false, null, function(index){
    	
    });
	
});

</script>
