function checkNum(str){
	var value = jQuery("#"+str+"").val();
	if(value=="")
		return false;
	else if(value=="0"){
		jQuery("#"+str+"").val("");
		return false;
	}else if(Number(value)<0){
		jQuery("#"+str+"").val("");
		return false;
	}
	if(!isDecimal(value)){
		alert("请输入正确的数据");
		jQuery("#"+str+"").val("");
		return true;
	}return false;
}
