<!-- 调拨单查询 -->

<table class='list'>
	<thead>
		<tr>
			<th>分配店</th>
			<th>调拨单号</th>
			<th>件数</th>
			<th>创建人</th>
			<th>创建日期</th>
			<th>状态</th>
		</tr>
	</thead>
	<tbody id='tbl'>
		#foreach($item in $form.moveBillList)
			<tr>
				<td>$!BdCommon.getOrgName($!item.orgId)</td>
				<td>$!item.moveNo</td>
				<td>$!item.sumCount</td>
				<td>$!BdCommon.getUserName($!item.createId)</td>
				<td>$!item.createDate</td>
				<td>$!DictUtil.getValue("status", $!item.status)</td>
			</tr>
		#end
	</tbody>
</table>
<script>
jQuery(function(){
	altRowCSS('tbl');
});
</script>