#set($jsfiles = ["$StringUtil.getContextPath()/js/calc/SpecialCharge.js?t=201204011204.js", 
	"$StringUtil.getContextPath()/dwr/interface/SpecialChargeDwr.js"])
<form id='frm' action="specialCharge.vm" method="post">
	#set($coefficient = $form.specialCharge)
	<input type="hidden" id="user_action" name="user_action" value=""/>
	<input type="hidden" id="chargeId" name="chargeId" value="$!{form.sc.chargeId}"/>
    <table class="title">
    	<tr><td>特殊工费核算规则表</td></tr>
    </table>
    <table class="control">
    	<tr><td>
				<input type='button' value='保存' class='btn' id='btnSave' $RightUtil.able($session, '08061602','1')>
				<input type='button' value='返回列表' class='btn' id="btnToList">
		</td></tr>
    </table>
    <table class='content'>
    	<tr>
			<td class='body1'>重量始</td>	
			<td class='body2'>
				<input type='text' id="weightStr" name='weightStr'  value="$!{form.sc.weightStr}" onblur="checkField()" dataType="Double" msg="不能为空，且必须是数字">
				$StringUtil.star()
			</td>
			<td class='body1'>重量止</td>	
			<td class='body2'>
				<input type='text' id="weightEnd" name='weightEnd'  value="$!{form.sc.weightEnd}" onblur="checkField()" dataType="Double" msg="不能为空，且必须是数字">
				$StringUtil.star()
			</td>
			<td class='body1'>核算重量</td>	
			<td class='body2'>
				<input type='text' id="weightCheck" name='weightCheck'  value="$!{form.sc.weightCheck}" onblur="checkField()" dataType="Double" msg="不能为空，且必须是数字">
				$StringUtil.star()
			</td>
			<td class='body1'>工费单价</td>	
			<td class='body2'>
				<input type='text' id="chargeUnit" name='chargeUnit'  value="$!{form.sc.chargeUnit}" onblur="checkField()" dataType="Double" msg="不能为空，且必须是数字">
				$StringUtil.star()
			</td>
		</tr>
		<tr>
			<td class='body1'>浮动值</td>	
			<td class='body2'>
				<input type='text' id="chargeFloat" name='chargeFloat'  value="$!{form.sc.chargeFloat}" onblur="checkField()" dataType="Double" msg="不能为空，且必须是数字">
				$StringUtil.star()
			</td>
			<td class='body1'>特殊工费</td>	
			<td class='body2'>
				<input type='text' id="chargeMoney" name='chargeMoney'  value="$!{form.sc.chargeMoney}" readonly dataType="Double" msg="不能为空，且必须是数字">
				$StringUtil.star()
			</td>
			<td class='body1'>状态</td>
			<td class='body2'>
				<input type='text' name='status' readonly value="保存">
			</td>
			<td class='body1'>备注</td>
			<td class='body2' colspan="5">
				<input type="text" id="memo" name="memo" style="width:90%;" value="$!{form.sc.memo}"/>
			</td>
		</tr>
    </table>
</form>
<script>

jQuery(function(){
	jQuery("#btnSave").click(saveform);
	
	jQuery("#btnToList").click(function(){
		window.location = "specialCharge.vm";
	});
})

</script>